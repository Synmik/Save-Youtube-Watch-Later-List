{
  "manifest_version": 3,
  "name": "YouTube Watch Later List",
  "version": "0.8.1",
  "description": "Save, export, and manage your YouTube watch later list",
  "permissions": ["storage", "tabs", "activeTab"],
  "host_permissions": [
    "https://www.youtube.com/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "YouTube Watch Later List",
    "default_icons": {
      "16": "YTPlaylistIcon.png",
      "24": "YTPlaylistIcon.png",
      "32": "YTPlaylistIcon.png"
    }
  },
  "icons": {
    "16": "YTPlaylistIcon.png",
    "48": "YTPlaylistIcon.png",
    "128": "YTPlaylistIcon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["scraper.js"],
      "run_at": "document_end"
    }
  ]
}